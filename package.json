    "isbot": "^3.6.5",
    "livekit-client": "^1.6.0",
    "livekit-server-sdk": "^1.1.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "react-icons": "^4.7.1",
    "react-player": "^2.11.0",
    "react-timeago": "^7.1.0",
